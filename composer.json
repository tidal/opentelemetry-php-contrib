{
    "name": "open-telemetry/opentelemetry-php-contrib",
    "description": "The contributor repo for opentelemetry-php",
    "type": "library",
    "license": "Apache-2.0",
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/open-telemetry/opentelemetry-php"
        }
      ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "open-telemetry/opentelemetry": "dev-main",
        "php-http/message": "^1.12",
        "php-http/discovery": "^1.14"
    },
    "authors": [
        {
            "name": "Bob Strecansky",
            "email": "bob.strecansky@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenTelemetry\\Instrumentation\\Symfony\\": "instrumentation/symfony"
        },
        "classmap": [
            "detectors/",
            "exporters/",
            "instrumentation/",
            "propagators/",
            "examples/"
        ] 
    },
    "autoload-dev": {
        "psr-4": {
            "OpenTelemetry\\Test\\Unit\\Symfony\\": "tests/unit/instrumentation/Symfony",
            "OpenTelemetry\\Test\\Integration\\Symfony\\": "tests/integration/instrumentation/Symfony"
        },
        "classmap": [
            "detectors/",
            "exporters/",
            "instrumentation/",
            "propagators/",
            "examples/"
        ] 
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "composer/xdebug-handler": "^2.0",
        "phan/phan": "^4.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "vimeo/psalm": "^4.0",
        "phpstan/phpstan": "^0.12.50",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "psalm/plugin-phpunit": "^0.13.0",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0@RC",
        "symfony/http-client": "^5.3",
        "nyholm/psr7": "^1.4",
        "symfony/config": "^4.4|^5.0|^6.0",
        "symfony/http-kernel": "^4.4|^5.3|^6.0",
        "symfony/polyfill-php80": "^1.16",
        "symfony/yaml": "^5.3",
        "symfony/proxy-manager-bridge": "^5.3",
        "symfony/options-resolver": "^5.3",
        "dg/bypass-finals": "^1.3",
        "kriswallsmith/buzz": "^1.2"
    }
}
